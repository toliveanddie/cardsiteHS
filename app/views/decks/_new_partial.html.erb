<div class="text-center">
</div>

<div class="row">
    <% hero_names(@herocards).each do |card| %>
        <div class="col-md-4">
            <%= form_for @deck do |f| %>
                <%= hidden_field_tag :name, card.playerClass %>
                <%= image_submit_tag card.img, style: "height: 350px;" %>
            <% end %>
        </div>
    <% end %>
    
</div>